{
  "name": "server-sunny",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "prod-start": "NODE_ENV=production node ./bin/www",
    "create-db": "NODE_ENV=production node_modules/.bin/sequelize db:create",
    "drop-db": "NODE_ENV=production node_modules/.bin/sequelize db:drop",
    "seed-categories": "NODE_ENV=production node_modules/.bin/sequelize db:seed --seed categories.js",
    "seed-articles": "NODE_ENV=production node_modules/.bin/sequelize db:seed --seed articles.js",
    "fetch_articles": "NODE_ENV=production node saveArticlesList.js"
  },
  "dependencies": {
    "aws-sdk": "^2.349.0",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "iconv-lite": "^0.4.24",
    "mime-types": "^2.1.21",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.1",
    "node-cron": "^2.0.3",
    "query-string": "^6.2.0",
    "request": "^2.88.0",
    "sanitize-html": "^1.19.1",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.2.0",
    "shelljs": "^0.8.2",
    "underscore": "^1.9.1",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "colors": "^1.3.2",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}

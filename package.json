{
  "name": "server-sunny",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "prod-start": "NODE_ENV=production node ./bin/www",
    "create-db": "NODE_ENV=production node_modules/.bin/sequelize db:create",
    "drop-db": "NODE_ENV=production node_modules/.bin/sequelize db:drop",
    "seed-categories": "NODE_ENV=production node_modules/.bin/sequelize db:seed --seed categories.js",
    "seed-articles": "NODE_ENV=production node_modules/.bin/sequelize db:seed --seed articles.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.1",
    "request": "^2.88.0",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}
